  font-size: 28rpx;
  color: #fd6e5e;
}

.navbar .item.active {
  color: #fff;
  background-color: #fd6e5e;
}

.vi {
  position: relative;
  top: 56rpx;
  border: 2rpx solid #e5e5e5;
}

/*总体主盒子*/

.container {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #fff;
  color: #939393;
}

/*左侧栏主盒子*/

.nav_left {
  position: absolute;
  left: -2px;
  top: 12rpx;
  width: 23%;
  height: -89rpx;
  background: #fff;
  text-align: center;
}

/*左侧栏list的item*/

.nav_left .nav_left_items {
  height: 106rpx;
  line-height: 86rpx;
  padding: 6px 0;
  font-size: 14px;
  color: #cccaca;
  background-color: rgb(250, 250, 252);
  margin-top: 20rpx;
}

/*左侧栏list的item被选中时*/

.nav_left .nav_left_items.active {
  background: #fff;
}

/*右侧栏主盒子*/

.nav_right {
  position: absolute;
  right: 2rpx;
  flex: 1;
  width: 77%;
  height: 500px;
  box-sizing: border-box;
  top: 15rpx;
}

/*右侧栏list的item*/

.nav_right .nav_right_items {
  float: left;
  width: 33.33%;
  text-align: center;
}

.nav_right .nav_right_items image {
  width: 84px;
  height: 103px;
  margin-top: 6px;
}

.nav_right .nav_right_items text {
  display: block;
  font-size: 14px;
  color: #9c9494;
  /*设置文字溢出部分为...*/
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.nodata_text {
  color: black;
  font-size: 14px;
  text-align: center;
}

