/* ----------------------------------- */
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.media {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 2px;
  left: 305px;
}

.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}

.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}

/* 搜索框模块 */

.weui-search-bar {
  position: fixed;
  z-index: 99999;
  width: 100%;
  top: 0px;
  padding: 32rpx 34rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  background-color: #fff;
}

.weui-icon-search {
  margin-right: 8px;
  font-size: inherit;
}

.weui-icon-search_in-box {
  position: absolute;
  left: 32rpx;
  top: 17rpx;
}

.weui-search-bar__text {
  display: inline-block;
  font-size: 14px;
  vertical-align: middle;
}

.weui-search-bar__form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
  flex: auto;
  border-radius: 10px;
  background: #fff;
  border: 2px solid #eee;
}

.weui-search-bar__box {
  position: relative;
  padding-left: 30px;
  padding-right: 30px;
  width: 85%;
  box-sizing: border-box;
  z-index: 1;
}

.weui-search-bar__input {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  padding-left: 18rpx;
}

.weui-icon-clear {
  position: absolute;
  top: 0;
  right: 0;
  padding: 7px 8px;
  font-size: 0;
}

.weui-search-bar__label {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 2;
  border-radius: 3px;
  text-align: center;
  color: #9b9b9b;
  background: #fff;
  line-height: 28px;
}

.weui-search-bar__cancel-btn {
  margin-left: 10px;
  line-height: 28px;
  color: #09bb07;
  white-space: nowrap;
}

.swiper-tab {
  z-index: 99999;
background-color: #fff;
position: fixed;
top: 58px;
width: 100%;
text-align: center;
line-height: 130rpx;
display: flex;
flex-direction: row;
justify-content: center;
height: 130rpx;
}

.tab-item {
  position: relative;
  flex: auto;
  text-align: center;
  height: 38rpx;
  line-height: 39rpx;
  top: 26rpx;
  border-radius: 54rpx;
  padding: 20rpx 20rpx;
  border: 2rpx solid #dfdede;
  left: 48rpx;
  margin: 0 90rpx 0 0;
  letter-spacing: 7rpx;
  font-size: 27rpx;
  color: #fd6e5e;
}

.on {
  color: #fff;
  background-color: #fd6e5e;
}

.swiper {
  position: absolute;
  top: 130px;
  display: block;
  height: 77%;
  width: 100%;
  overflow: hidden;
}

swiper-item {
  height: 100%;
  overflow: scroll;
  overflow-x: hidden;
}

scroll-view {
  height: 100%;
  overflow: auto;
  white-space: nowrap;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/* 导航栏 */

.navbar .item {
  position: relative;
  flex: auto;
  text-align: center;
